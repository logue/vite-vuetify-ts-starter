{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "vite-vuetify-ts-starter",
  "description": "Vue3 Vuetify TypeScript Startar project for Vite.",
  "version": "1.13.6",
  "license": "MIT",
  "type": "module",
  "private": true,
  "author": {
    "name": "Logue",
    "email": "logue@hotmail.co.jp",
    "url": "https://logue.dev/"
  },
  "homepage": "https://github.com/logue/vite-vuetify-ts-starter",
  "repository": {
    "type": "git",
    "url": "git@github.com:logue/vite-vuetify-ts-starter.git"
  },
  "bugs": {
    "url": "https://github.com/logue/vite-vuetify-ts-starter/issues"
  },
  "engines": {
    "node": ">=22.0.1",
    "pnpm": ">=9.15.0"
  },
  "packageManager": "pnpm@9.15.2",
  "scripts": {
    "dev": "vite",
    "clean": "rimraf ./node_modules/.vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "build:analyze": "vite build --mode analyze",
    "build:clean": "rimraf dist",
    "lint": "eslint . --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint && prettier . -w -u",
    "lint:style": "stylelint \"./**/*.{css,sass,scss,htm,html,vue}\" --fix --cache-location ./node_modules/.vite/vite-plugin-stylelint && prettier \"./**/*.{css,sass,scss,htm,html,vue}\" -w -u",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "prepare": "husky"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "core-js": "^3.39.0",
    "pinia": "^2.3.0",
    "pinia-plugin-persistedstate": "^4.2.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vuetify": "^3.7.6",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@playwright/test": "^1.49.1",
    "@rushstack/eslint-patch": "^1.10.4",
    "@tsconfig/node-lts": "^22.0.1",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.10.5",
    "@types/webfontloader": "^1.6.38",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/coverage-v8": "^2.1.8",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.2.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.17.0",
    "eslint-import-resolver-custom-alias": "^1.3.2",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-playwright": "^2.1.0",
    "eslint-plugin-tsdoc": "^0.4.0",
    "eslint-plugin-vue": "^9.32.0",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "eslint-plugin-vuetify": "^2.5.1",
    "eslint-plugin-yaml": "^1.0.3",
    "husky": "^9.1.7",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.3.0",
    "npm-run-all2": "^7.0.2",
    "postcss-html": "^1.7.0",
    "prettier": "^3.4.2",
    "resize-observer-polyfill": "^1.5.1",
    "rimraf": "^6.0.1",
    "rollup-plugin-visualizer": "^5.13.1",
    "sass": "^1.83.1",
    "stylelint": "^16.12.0",
    "stylelint-config-recommended-scss": "^14.1.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-order": "^6.0.4",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.19.0",
    "vite": "^6.0.7",
    "vite-plugin-checker": "0.8.0",
    "vite-plugin-vue-devtools": "^7.6.8",
    "vite-plugin-vuetify": "^2.0.4",
    "vitest": "^2.1.8",
    "vue-tsc": "^2.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,json,yaml,yml,vue,htm,html,md}": "eslint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint",
    "*.{css,sass,scss,vue,htm,html}": "stylelint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-stylelint",
    "*": "prettier -w -u"
  },
  "resolutions": {
    "json5": ">=2.2.3",
    "yaml": ">=2.3.2"
  },
  "stackblitz": {
    "startCommand": "pnpm run test:unit"
  }
}
